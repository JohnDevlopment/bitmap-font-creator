; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[BitmapFontPlugin]

debug/print_messages=true

[application]

config/name="Bitmap Font Creator Plugin"
run/main_scene="res://Main.tscn"
config/icon="res://icon.png"

[autoload]

NodeMapper="*res://addons/node_mapper/NodeMapper.gd"

[debug]

gdscript/warnings/return_value_discarded=false

[editor_plugins]

enabled=PoolStringArray( "res://addons/Todo_Manager/plugin.cfg", "res://addons/bitmap_font_creator/plugin.cfg", "res://addons/godot-plugin-refresher/plugin.cfg", "res://addons/node_mapper/plugin.cfg" )

[importer_defaults]

texture={
"compress/normal_map": 2,
"detect_3d": false
}

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
